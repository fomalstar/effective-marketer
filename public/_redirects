# Serve static blog HTML files directly
/blog/*.html    /blog/:splat.html    200

# Redirect blog URLs without .html to .html version (catch-all for blog posts)
/blog/*    /blog/:splat.html    301

# Fallback to React Router for non-blog routes
/    /index.html   200
/google-autosuggest-ranking    /index.html   200
/lead-gen-ai-automation    /index.html   200
/onboarding    /index.html   200
/login    /index.html   200
/blog-admin    /index.html   200
/sitemap.xml    /sitemap.xml   200
/rss.xml    /rss.xml   200