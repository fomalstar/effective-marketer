{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build:client": "vite build --outDir dist/client --ssrManifest",
    "build:server": "vite build --outDir dist/server --ssr src/entry-server.jsx",
    "build": "vite build && node copy-page-content.cjs",
    "build:static": "vite build",
    "build:render": "node build.js",
    "start": "node server-setup/express-server.cjs",
    "start:backend": "node server-setup/ssr-server.mjs",
    "serve": "cross-env NODE_ENV=production node server-setup/ssr-server.mjs",
    "lint": "eslint .",
    "preview": "vite preview",
    "indexnow": "node scripts/indexnow-submit.mjs --host=$SITE_URL",
    "sitemap": "node scripts/generate-sitemap.mjs",
    "deploy:static": "npm run sitemap && npm run build",
    "deploy:backend": "npm run build && npm run start:backend"
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^7.6.3",
    "sirv": "^2.0.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/node": "^24.3.0",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "cross-env": "^7.0.3",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.20.5",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^7.1.3"
  }
}
